/*---------------------------------------------
-- 오브젝트명: board.sa_member
-- 생성일자 : 2022-04-12 17:48:07.0
-- 상태: VALID
---------------------------------------------*/
CREATE TABLE `board`.`member_table`
(
    `id`    int(20)      NOT NULL AUTO_INCREMENT,
    `memberEmail`    varchar(20)  CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL,
    `memberPassword`    varchar(10)  CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL,
    `memberName`  varchar(20)  CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL,
    PRIMARY KEY (`id`)
)
ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci ROW_FORMAT=Dynamic AUTO_INCREMENT=4;


